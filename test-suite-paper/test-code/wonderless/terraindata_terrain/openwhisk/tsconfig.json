{
    "compilerOptions": {
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "target": "es6",
        "typeRoots": [
            "./node_modules/@types"
        ],
        "outDir": "./build/",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "rootDir": "./"
    },
    "files": [
        "Benchmark.ts",
        "runBench.ts"
    ],
    "exclude": [
        "node_modules",
        "build"
    ]
}
